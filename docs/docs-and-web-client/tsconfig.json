{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$components/*": ["./src/components/*"],
      "~/*": ["./src/*"],
      "lipilekhika/*": ["../../packages/js/src/*"],
      "lipilekhika": ["../../packages/js/src/index.ts"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "exclude": ["dist"]
}
